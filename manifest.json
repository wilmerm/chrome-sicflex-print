{
    "name": "Sicflex Print",
    "version": "3.4",
    "description": "Extenci√≥n para imprimir facturas en Sicflex desde el navegador.",
    "author": "unolet.com",

    "permissions": [
        "declarativeContent",
        "storage",
        "https://*.sicflex.com/*",
        "https://demo.unolet.com/",
        "https://unolet.com/api/genericcounter-add/"
    ],

    "content_scripts": [
        {
          "matches": ["https://*.sicflex.com/*"],
          "css": ["background.css"],
          "js": ["content.js"]
      }
    ],

    "web_accessible_resources": [
        "background.css",
        "print.html",
        "print.css",
        "images/*"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },

    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/print16.png",
            "32": "images/print32.png",
            "48": "images/print48.png",
            "128": "images/print128.png"
          }
    },

    "icons": {
        "16": "images/print16.png",
        "32": "images/print32.png",
        "48": "images/print48.png",
        "128": "images/print128.png"
    },

    "manifest_version": 2
}